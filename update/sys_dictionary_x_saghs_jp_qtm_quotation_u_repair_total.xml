        <attributes/>
        <audit>false</audit>
        <calculation><![CDATA[(function calculatedFieldValue(current) {
	
	var rt;
	var ef;
	var phf;

	if (current.u_repair_type && current.u_parts_total > 0) {
		ef = current.u_repair_type.u_shotaiouhi;
		phf = current.u_repair_type.u_buhintehaihi;
		rt = parseFloat(ef) + parseFloat(phf);
	}
	
	else if (current.u_repair_type && current.u_parts_total == 0) {
		ef = current.u_repair_type.u_shotaiouhi;
		rt =  parseFloat(ef);
	}
	
	else {
		rt = parseFloat(0);
	}
	
	return rt;  // return the calculated value

})(current);]]></calculation>
